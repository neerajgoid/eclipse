package com.globant.neeraj.assignments;

import java.util.ArrayList;
import java.util.Iterator;

public class Main {

	public static void main(String[] args) {
		Employee neeraj = new Employee("Neeraj", 1, 20);
		Employee akash = new Employee("Akash", 1, 30);
		Employee anirudh = new Employee("Anirudh", 1, 25);
		Employee neeraj1 = new Employee("Neeraj",2,25);
		
		
		ArrayList<Employee> al = new ArrayList<>();
		al.add(neeraj);
		al.add(neeraj1);
		al.add(anirudh);
		al.add(anirudh);
		al.add(akash);
		
		//Iterator<al> itr = new Iterator<>(); 
		Iterator<Employee> itr = al.iterator();
		
		
		while(itr.hasNext())
		{
			Employee e = (Employee) itr.next();
			System.out.println("Employee details: Name:" + e.getName() +"Age:"+ e.getAge() +"Id:"+ e.getId());
		}
		

	}

}
