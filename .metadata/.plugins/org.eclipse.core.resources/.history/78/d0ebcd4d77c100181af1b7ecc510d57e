package treemapp;

public class TmEmployee {

private	String name;
private	int salary;

	public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public int getSalary() {
	return salary;
}
public void setSalary(int salary) {
	this.salary = salary;
}
	
	TmEmployee(String name,int salary)
	{
		this.name=name;
		this.salary=salary;
	}
	
	
}
